{
  "name": "redux-blueprint",
  "version": "0.4.3",
  "description": "Abstracted actions for redux (WIP)",
  "main": "lib/index.js",
  "types": "index.d.ts",
  "scripts": {
    "start": "npm run build -- --watch",
    "prebuild": "rimraf lib",
    "build": "tsc --project tsconfig.build.json --rootDir src --outDir lib",
    "test": "jest",
    "preversion": "npm test"
  },
  "dependencies": {
    "@types/redux-actions": "^2.2.2",
    "invariant": "^2.2.2",
    "redux-actions": "^2.2.1",
    "tslib": "^1.8.0"
  },
  "devDependencies": {
    "@types/invariant": "^2.2.29",
    "@types/jest": "^21.1.6",
    "@types/node": "^8.0.52",
    "@types/sinon": "^4.0.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.1",
    "jest": "^21.2.1",
    "react": "^16.0.0",
    "rimraf": "^2.6.2",
    "sinon": "^4.1.2",
    "ts-jest": "^21.2.2",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testRegex": "/__tests__/.*\\.test\\.(ts|tsx)$",
    "globals": {
      "__JEST__": true,
      "ts-jest": {
        "useBabelrc": true
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cchamberlain/redux-blueprint.git"
  },
  "keywords": [
    "redux",
    "react",
    "idle",
    "monitor"
  ],
  "author": "Cole Chamberlain <cole.chamberlain@gmail.com> (https://github.com/cchamberlain)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cchamberlain/redux-blueprint/issues"
  },
  "homepage": "https://cchamberlain.github.io/redux-blueprint"
}
