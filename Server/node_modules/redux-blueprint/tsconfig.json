{
  "compilerOptions": {
    "experimentalDecorators": true,
    "target": "ES5",
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react",
    "noImplicitReturns": true,
    "sourceMap": false,
    "allowSyntheticDefaultImports": false,
    "importHelpers": true,
    "typeRoots": ["./node_modules/@types"],
    "lib": [
      "es2017",
      "dom"
    ]
  },
  "exclude": [ "node_modules" ]
}
